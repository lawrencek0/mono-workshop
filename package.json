{
  "name": "mean-hero",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "concurrently --kill-others \"ng serve --aot=true --proxy-config proxy.conf.json\" \"npm run _server:run\"",
    "lint": "tslint \"src/**/*.ts\"",
    "test": "ng test --watch=false",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build --prod --sm=false --aot=true --output-path=dist/client && npm run _server:build",
    "_server:run": "tsc -p ./server && concurrently \"tsc -w -p ./server\" \"nodemon dist/server/bin/www.js\" ",
    "_server:build": "tsc -p ./server",
    "postinstall": "npm run build"
  },
  "private": true,
  "dependencies": {
    "@angular/cli": "1.0.0-beta.31",
    "@angular/common": "^2.3.1",
    "@angular/compiler": "^2.3.1",
    "@angular/compiler-cli": "^2.3.1",
    "@angular/core": "^2.3.1",
    "@angular/forms": "^2.3.1",
    "@angular/http": "^2.3.1",
    "@angular/platform-browser": "^2.3.1",
    "@angular/platform-browser-dynamic": "^2.3.1",
    "@angular/router": "^3.3.1",
    "@types/body-parser": "0.0.33",
    "@types/express": "^4.0.35",
    "@types/node": "^6.0.62",
    "@webcomponents/custom-elements": "^1.0.0-alpha.3",
    "body-parser": "^1.16.0",
    "clarity-angular": "^0.8.6",
    "clarity-icons": "^0.8.6",
    "clarity-ui": "^0.8.6",
    "core-js": "^2.4.1",
    "express": "^4.14.1",
    "jquery": "^3.1.1",
    "mutationobserver-shim": "^0.3.2",
    "path": "^0.12.7",
    "rxjs": "^5.0.1",
    "slick-carousel": "^1.6.0",
    "ts-helpers": "^1.1.1",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.42",
    "@types/jquery": "^2.0.40",
    "@types/node": "^7.0.5",
    "@types/slick-carousel": "^1.6.31",
    "codelyzer": "~2.0.0-beta.1",
    "concurrently": "^3.1.0",
    "jasmine-core": "2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "1.4.1",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.0.2",
    "karma-remap-istanbul": "^0.6.0",
    "nodemon": "^1.11.0",
    "protractor": "~5.1.0",
    "ts-node": "2.1.0",
    "tslint": "^4.3.0",
    "typescript": "~2.1.5"
  },
  "engines": {
    "node": "7.2.0"
  }
}
